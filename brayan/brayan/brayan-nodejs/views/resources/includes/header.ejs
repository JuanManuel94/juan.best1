<div id="header" class="header navbar-default">
    <div class="navbar-header">
        <a href="/dashboard" class="navbar-brand">
            <% let logoSystem='/public/images/logotypes/superwisp_white.png' ; if (typeof business !=='undefined' &&
                business && business.logo_system) { if (business.logo_system !=='superwisp_white.png' ) {
                logoSystem='/public/uploads/business/' + business.logo_system; } } %>
                <img src="<%= logoSystem %>" alt="Logo" style="height: 30px;">
        </a>
        <button type="button" class="navbar-toggle" data-click="sidebar-toggled">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
    </div>
    <ul class="navbar-nav navbar-right">
        <li class="dropdown navbar-user">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                <% let headerUserImage='/public/images/user/default.png' ; if (typeof userData !=='undefined' &&
                    userData && userData.image) { if (userData.image !=='default.png' ) {
                    headerUserImage='/public/uploads/users/' + userData.image; } } %>
                    <img src="<%= headerUserImage %>" alt="">
                    <span class="d-none d-md-inline">
                        <% if (typeof userData !=='undefined' && userData) { %>
                            <%= userData.names || '' %>
                                <%= userData.surnames || '' %>
                                    <% } else { %>
                                        Usuario
                                        <% } %>
                    </span>
                    <b class="caret"></b>
            </a>
            <div class="dropdown-menu dropdown-menu-right">
                <a href="javascript:;" class="dropdown-item">Mi perfil</a>
                <div class="dropdown-divider"></div>
                <a href="/logout" class="dropdown-item">Cerrar sesi√≥n</a>
            </div>
        </li>
    </ul>
</div>